{
    "name": "dodopayments/laravel-dodopayments",
    "description": "Official Laravel integration for DodoPayments - Accept payments with ease",
    "keywords": [
        "laravel",
        "dodopayments",
        "payment",
        "checkout",
        "subscription",
        "billing"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "DodoPayments",
            "email": "[email protected]"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.5|^10.0"
    },
    "autoload": {
        "psr-4": {
            "DodoPayments\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DodoPayments\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DodoPayments\\Laravel\\DodoPaymentsServiceProvider"
            ],
            "aliases": {
                "DodoPayments": "DodoPayments\\Laravel\\Facades\\DodoPayments"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
